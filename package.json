{
  "name": "notejs-react",
  "version": "1.0.0",
  "scripts": {
    "build": "rm -fr public && gulp build && npm run js:build ",
    "deploy": "npm test && npm run build && surge public",
    "js:build": "NODE_ENV=production webpack",
    "js:watch": "mkdir -p public/js && watchify -p [ browserify-hmr -h 0.0.0.0 -m none --supportModes [ websocket ] ] src/js/app.js -o public/js/app.js -v",
    "lint": "eslint .",
    "lint:watch": "esw -w",
    "prestart": "rm -fr public && mkdir -p public/js",
    "start": "parallelshell 'gulp' 'npm run js:watch' 'npm run lint:watch' 'npm run webserver'",
    "test": "npm run lint && NODE_ENV=test mocha --opts ./mocha.opts",
    "test:watch": "npm test -- --watch",
    "webserver": "live-server --ignore=js --no-browser --port=3000 public"
  },
  "author": "Kenneth Chung",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "6.2.2",
    "babel": "5.8.23",
    "babel-core": "5.8.25",
    "babel-loader": "5.3.2",
    "babel-plugin-auto-symbol-description": "0.0.1",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-react-transform": "1.1.1",
    "babel-runtime": "5.8.25",
    "babelify": "6.3.0",
    "browserify": "12.0.1",
    "browserify-hmr": "0.3.1",
    "classnames": "2.2.1",
    "core-js": "2.0.0",
    "cssnext": "1.8.4",
    "dom-storage": "2.0.1",
    "eslint": "1.10.3",
    "eslint-config-kentor": "1.0.0",
    "eslint-plugin-react": "3.13.1",
    "eslint-watch": "2.1.4",
    "expect": "1.13.4",
    "fastclick": "1.0.6",
    "firebase": "2.3.2",
    "gulp": "3.9.0",
    "gulp-minify-css": "1.2.2",
    "gulp-postcss": "6.0.1",
    "gulp-sourcemaps": "1.6.0",
    "hammerjs": "2.0.4",
    "history": "1.17.0",
    "immutable": "3.7.6",
    "jsdom": "7.2.2",
    "live-server": "0.9.0",
    "marked": "0.3.5",
    "mocha": "2.3.4",
    "normalize.css": "3.0.3",
    "parallelshell": "2.0.0",
    "postcss-import": "7.1.3",
    "react": "0.14.3",
    "react-addons-css-transition-group": "0.14.3",
    "react-addons-linked-state-mixin": "0.14.3",
    "react-addons-pure-render-mixin": "0.14.3",
    "react-addons-test-utils": "0.14.3",
    "react-dom": "0.14.3",
    "react-hot-loader": "1.3.0",
    "react-redux": "4.0.5",
    "react-router": "1.0.3",
    "react-timeago": "2.2.1",
    "react-transform-hmr": "1.0.1",
    "redux": "3.0.5",
    "redux-logger": "2.3.1",
    "sinon": "1.17.2",
    "surge": "0.17.4",
    "watchify": "3.6.1",
    "webpack": "1.12.9"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
