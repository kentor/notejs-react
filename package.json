{
  "name": "notejs-react",
  "version": "1.0.0",
  "scripts": {
    "build": "rm -fr public && gulp build && npm run js:build ",
    "deploy": "npm test && npm run build && surge public",
    "js:build": "NODE_ENV=production webpack",
    "js:watch": "mkdir -p public/js && watchify -p [ browserify-hmr -h 0.0.0.0 -m none --supportModes [ websocket ] ] src/js/app.js -o public/js/app.js -v",
    "lint": "eslint .",
    "lint:watch": "esw -w",
    "prestart": "rm -fr public && mkdir -p public/js",
    "start": "parallelshell 'gulp' 'npm run js:watch' 'npm run lint:watch' 'npm run webserver'",
    "test": "npm run lint && NODE_ENV=test mocha --opts ./mocha.opts",
    "test:watch": "npm test -- --watch",
    "webserver": "live-server --ignore=js --no-browser --port=3000 public"
  },
  "author": "Kenneth Chung",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.7.6",
    "babel-loader": "6.2.4",
    "babel-plugin-auto-symbol-description": "1.0.0",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-react-display-name": "6.5.0",
    "babel-plugin-transform-runtime": "6.7.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.7.2",
    "babel-runtime": "6.6.1",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "browserify-hmr": "0.3.1",
    "classnames": "2.2.3",
    "core-js": "2.2.2",
    "dom-storage": "2.0.2",
    "eslint": "2.7.0",
    "eslint-config-kentor": "2.0.0",
    "eslint-plugin-react": "4.3.0",
    "eslint-watch": "2.1.10",
    "expect": "1.16.0",
    "firebase": "2.4.2",
    "gulp": "3.9.1",
    "gulp-minify-css": "1.2.4",
    "gulp-postcss": "6.1.0",
    "gulp-sourcemaps": "1.6.0",
    "hammerjs": "2.0.6",
    "history": "2.0.1",
    "immutable": "3.7.6",
    "jsdom": "8.3.1",
    "live-server": "0.9.2",
    "marked": "0.3.5",
    "mocha": "2.4.5",
    "normalize.css": "4.1.1",
    "parallelshell": "2.0.0",
    "postcss-cssnext": "2.5.2",
    "postcss-import": "8.1.0",
    "react": "15.0.1",
    "react-addons-css-transition-group": "15.0.1",
    "react-addons-linked-state-mixin": "15.0.1",
    "react-addons-pure-render-mixin": "15.0.1",
    "react-addons-test-utils": "15.0.1",
    "react-dom": "15.0.1",
    "react-hot-loader": "1.3.0",
    "react-redux": "4.4.5",
    "react-router": "2.2.2",
    "react-timeago": "3.0.0-beta.3",
    "react-transform-hmr": "1.0.4",
    "redux": "3.4.0",
    "redux-logger": "2.6.1",
    "sinon": "1.17.3",
    "surge": "0.17.7",
    "watchify": "3.7.0",
    "webpack": "1.12.15"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
