{
  "name": "notejs-react",
  "version": "1.0.0",
  "scripts": {
    "build": "rm -fr public && gulp build && npm run js:build ",
    "deploy": "npm test && npm run build && surge public",
    "js:build": "NODE_ENV=production webpack",
    "js:watch": "mkdir -p public/js && watchify -p [ browserify-hmr -h 0.0.0.0 -m none --supportModes [ websocket ] ] src/js/app.js -o public/js/app.js -v",
    "lint": "eslint .",
    "lint:watch": "esw -w",
    "prestart": "rm -fr public && mkdir -p public/js",
    "start": "parallelshell 'gulp' 'npm run js:watch' 'npm run lint:watch' 'npm run webserver'",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "webserver": "live-server --ignore=js --no-browser --port=3000 public"
  },
  "author": "Kenneth Chung",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.16.0",
    "babel-jest": "15.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-auto-symbol-description": "1.0.0",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-react-display-name": "6.8.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-runtime": "6.11.6",
    "babelify": "7.3.0",
    "browserify": "13.1.0",
    "browserify-hmr": "0.3.5",
    "classnames": "2.2.5",
    "core-js": "2.4.1",
    "dom-storage": "2.0.2",
    "enzyme": "2.4.1",
    "eslint": "3.6.1",
    "eslint-config-kentor": "3.0.1",
    "eslint-plugin-react": "6.3.0",
    "eslint-watch": "2.1.14",
    "expect": "1.20.2",
    "firebase": "2.4.2",
    "gulp": "3.9.1",
    "gulp-minify-css": "1.2.4",
    "gulp-postcss": "6.2.0",
    "gulp-sourcemaps": "2.0.0-alpha",
    "hammerjs": "2.0.8",
    "immutable": "3.8.1",
    "jest": "15.1.1",
    "live-server": "1.1.0",
    "marked": "0.3.6",
    "normalize.css": "4.2.0",
    "parallelshell": "2.0.0",
    "postcss-cssnext": "2.8.0",
    "postcss-import": "8.1.2",
    "postcss-url": "5.1.2",
    "react": "15.3.2",
    "react-addons-css-transition-group": "15.3.2",
    "react-addons-linked-state-mixin": "15.3.2",
    "react-addons-pure-render-mixin": "15.3.2",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-timeago": "3.1.3",
    "react-transform-hmr": "1.0.4",
    "redux": "3.6.0",
    "redux-logger": "2.6.1",
    "sinon": "1.17.6",
    "surge": "0.18.0",
    "watchify": "3.7.0",
    "webpack": "1.13.2"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "jest": {
    "testRegex": "/__tests__/.*?-test\\.js$"
  }
}
