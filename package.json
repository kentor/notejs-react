{
  "name": "notejs-react",
  "version": "1.0.0",
  "scripts": {
    "build": "rm -fr public && gulp build && npm run js:build ",
    "deploy": "npm test && npm run build && firebase deploy",
    "js:build": "NODE_ENV=production webpack",
    "js:watch": "mkdir -p public/js && watchify -p [ browserify-hmr -h 0.0.0.0 -m none --supportModes [ websocket ] ] src/js/app.js -o public/js/app.js -v",
    "lint": "eslint .",
    "lint:watch": "esw -w",
    "prestart": "rm -fr public && mkdir -p public/js",
    "start": "parallelshell 'gulp' 'npm run js:watch' 'npm run lint:watch' 'npm run webserver'",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "webserver": "live-server --ignore=js --no-browser --port=3000 public"
  },
  "author": "Kenneth Chung",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.1",
    "babel-jest": "19.0.0",
    "babel-loader": "6.4.1",
    "babel-plugin-auto-symbol-description": "1.0.0",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-react-display-name": "6.23.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.23.0",
    "babelify": "7.3.0",
    "browserify": "14.3.0",
    "browserify-hmr": "0.3.5",
    "classnames": "2.2.5",
    "core-js": "2.4.1",
    "dom-storage": "2.0.2",
    "enzyme": "2.8.0",
    "eslint": "3.19.0",
    "eslint-config-kentor": "3.14.0",
    "eslint-plugin-react": "6.10.3",
    "eslint-watch": "3.0.1",
    "expect": "1.20.2",
    "firebase": "2.4.2",
    "firebase-tools": "3.6.0",
    "gulp": "3.9.1",
    "gulp-minify-css": "1.2.4",
    "gulp-postcss": "6.4.0",
    "gulp-sourcemaps": "2.5.1",
    "hammerjs": "2.0.8",
    "immutable": "3.8.1",
    "jest": "19.0.2",
    "live-server": "1.2.0",
    "marked": "0.3.6",
    "normalize.css": "6.0.0",
    "parallelshell": "2.0.0",
    "postcss-cssnext": "2.10.0",
    "postcss-import": "9.1.0",
    "postcss-url": "6.0.4",
    "prop-types": "15.5.4",
    "react": "15.5.3",
    "react-addons-css-transition-group": "15.5.2",
    "react-addons-test-utils": "15.5.1",
    "react-dom": "15.5.3",
    "react-redux": "5.0.3",
    "react-timeago": "3.2.0",
    "react-transform-hmr": "1.0.4",
    "redux": "3.6.0",
    "redux-logger": "3.0.1",
    "sinon": "2.1.0",
    "watchify": "3.9.0",
    "webpack": "2.3.3"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "jest": {
    "testRegex": "/__tests__/.*?-test\\.js$"
  }
}
