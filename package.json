{
  "name": "notes-ts",
  "version": "0.0.0",
  "scripts": {
    "build": "yarn clean && yarn js:build && yarn static:build",
    "clean": "rm -fr public && mkdir -p public",
    "gen-icons": "babel-node --extensions '.ts' src/scripts/gen-icons",
    "js:build": "webpack --config webpack.prod.js",
    "js:watch": "webpack-dev-server --config webpack.dev.js",
    "lint": "eslint . --ext=.js,.ts,.tsx",
    "npmsc": "./node_modules/npmsc/lib/cli.js",
    "prestart": "yarn clean",
    "start": "yarn npmsc init js:watch static:watch",
    "static:build": "copy-and-watch 'src/static/**/*' public",
    "static:watch": "yarn static:build --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc",
    "webserver": "live-server --entry-file=index.html --ignore=js --no-browser --port=10000 public"
  },
  "private": true,
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@hot-loader/react-dom": "^16.8.6",
    "@types/jest": "^24.0.0",
    "@types/marked": "^0.6.0",
    "@types/node": "^11.9.0",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-timeago": "^4.1.0",
    "@types/react-transition-group": "^2.0.16",
    "@types/styled-jsx": "^2.2.7",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "copy-and-watch": "^0.1.2",
    "eslint": "^5.13.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.1",
    "firebase": "^5.8.2",
    "io-ts": "^1.8.2",
    "jest": "^24.1.0",
    "live-server": "^1.2.1",
    "marked": "^0.6.0",
    "prettier": "^1.16.4",
    "react": "16.8.4",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "^4.6.5",
    "react-testing-library": "^6.0.0",
    "react-timeago": "^4.3.0",
    "react-transition-group": "^2.5.3",
    "redux": "^4.0.1",
    "redux-devtools": "^3.5.0",
    "redux-react-hook": "^3.1.0",
    "styled-jsx": "^3.2.1",
    "typescript": "^3.3.3",
    "webpack": "^4.29.3",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  }
}
