{
  "name": "notejs-react",
  "version": "1.0.0",
  "scripts": {
    "build": "rm -fr public && gulp build && gulp processhtml && npm run js:build ",
    "deploy": "./deploy",
    "js:build": "NODE_ENV=production webpack",
    "js:watch": "webpack-dev-server --config webpack.hot.config.js --port 45537 --hot --inline --no-info",
    "lint": "eslint .",
    "lint:watch": "esw -w",
    "prestart": "rm -fr public && mkdir public",
    "start": "gulp & npm run js:watch & npm run lint:watch & npm run webserver",
    "test": "npm run lint && NODE_ENV=test mocha --opts ./mocha.opts",
    "test:watch": "npm test -- --watch",
    "webserver": "live-server --port=3000 --no-browser public"
  },
  "author": "Kenneth Chung",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.0.2",
    "babel": "^5.8.23",
    "babel-core": "^5.8.24",
    "babel-loader": "^5.3.2",
    "babel-plugin-auto-symbol-description": "0.0.1",
    "babel-runtime": "^5.8.24",
    "classnames": "^2.1.3",
    "core-js": "^1.1.4",
    "cssnext": "^1.8.4",
    "dom-storage": "^2.0.1",
    "eslint": "^1.4.1",
    "eslint-plugin-react": "^3.3.2",
    "eslint-watch": "^2.0.0",
    "expect": "^1.10.0",
    "fastclick": "^1.0.6",
    "firebase": "^2.2.9",
    "gulp": "^3.9.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-postcss": "^6.0.0",
    "gulp-processhtml": "^1.1.0",
    "gulp-sourcemaps": "^1.5.2",
    "hammerjs": "^2.0.4",
    "immutable": "^3.7.5",
    "jsdom": "3.x.x",
    "live-server": "^0.8.1",
    "marked": "^0.3.5",
    "mocha": "^2.3.2",
    "normalize.css": "^3.0.3",
    "postcss-import": "^7.0.0",
    "react": "^0.14.0-rc1",
    "react-addons-css-transition-group": "^0.14.0-rc1",
    "react-addons-linked-state-mixin": "^0.14.0-rc1",
    "react-addons-pure-render-mixin": "^0.14.0-rc1",
    "react-dom": "^0.14.0-rc1",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^2.1.2",
    "react-router": "^1.0.0-rc1",
    "react-timeago": "git://github.com/kentor/react-timeago.git",
    "redux": "^3.0.0",
    "redux-logger": "^1.0.6",
    "sinon": "^1.16.1",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.10.1"
  }
}
