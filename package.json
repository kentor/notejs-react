{
  "name": "notejs-react",
  "version": "1.0.0",
  "scripts": {
    "build": "rm -fr public && gulp build && npm run js:build ",
    "deploy": "npm test && npm run build && firebase deploy",
    "gulp": "gulp",
    "js:build": "NODE_ENV=production webpack",
    "js:watch": "mkdir -p public/js && watchify -p [ browserify-hmr -h 0.0.0.0 -m none --supportModes [ websocket ] ] src/js/app.js -o public/js/app.js -v",
    "lint": "eslint .",
    "lint:watch": "esw -w --changed",
    "prestart": "rm -fr public && mkdir -p public/js",
    "start": "npm-run-all -p gulp js:watch lint:watch webserver",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "webserver": "live-server --ignore=js --no-browser --port=3000 public"
  },
  "author": "Kenneth Chung",
  "license": "MIT",
  "dependencies": {
    "babel-core": "6.26.0",
    "babel-env": "2.4.1",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-auto-symbol-description": "1.0.0",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-react-display-name": "6.25.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.26.0",
    "babelify": "7.3.0",
    "browserify": "14.3.0",
    "browserify-hmr": "0.3.5",
    "classnames": "2.2.5",
    "core-js": "2.4.1",
    "dom-storage": "2.0.2",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.1",
    "eslint": "4.8.0",
    "eslint-config-kentor": "4.0.3",
    "eslint-plugin-react": "7.4.0",
    "eslint-watch": "3.1.2",
    "firebase": "2.4.2",
    "firebase-tools": "3.6.0",
    "gulp": "3.9.1",
    "gulp-minify-css": "1.2.4",
    "gulp-postcss": "6.4.0",
    "gulp-sourcemaps": "2.5.1",
    "hammerjs": "2.0.8",
    "immutable": "3.8.1",
    "jest": "21.2.1",
    "live-server": "1.2.0",
    "marked": "0.3.6",
    "normalize.css": "6.0.0",
    "npm-run-all": "4.1.1",
    "postcss-cssnext": "2.10.0",
    "postcss-import": "9.1.0",
    "postcss-url": "6.0.4",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-addons-css-transition-group": "15.6.2",
    "react-addons-test-utils": "15.6.2",
    "react-dom": "16.0.0",
    "react-redux": "5.0.6",
    "react-test-renderer": "16.0.0",
    "react-timeago": "3.4.3",
    "react-transform-hmr": "1.0.4",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "sinon": "4.0.1",
    "watchify": "3.9.0",
    "webpack": "2.3.3"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "testRegex": "/__tests__/.*?-test\\.js$"
  }
}
