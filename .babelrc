{
  "env": {
    "development": {
      "plugins": [
        "auto-symbol-description",
        "react-display-name",
        "react-transform"
      ],
      "extra": {
        "react-transform": {
          "transforms": [
            {
              "transform": "react-transform-hmr",
              "imports": ["react"],
              "locals": ["module"]
            }
          ]
        }
      }
    }
  },
  "optional": ["runtime"]
}
