{
  "presets": ["es2015", "react"],
  "env": {
    "development": {
      "plugins": [
        ["auto-symbol-description"],
        ["react-transform", {
          "transforms": [
            {
              "transform": "react-transform-hmr",
              "imports": ["react"],
              "locals": ["module"]
            }
          ]
        }],
        ["transform-react-display-name"]
      ]
    }
  },
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ]
}
